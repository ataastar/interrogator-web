    "rimraf": "^3.0.0",
    "rollup": "^1.29.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.4",
    "source-map-explorer": "^2.2.2",
    "style-loader": "^1.1.3",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
